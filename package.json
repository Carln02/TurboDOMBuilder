{
  "name": "turbodombuilder",
  "version": "0.6.1",
  "description": "A lightweight JavaScript library for quickly creating and appending DOM elements.",
  "type": "module",
  "main": "build/turbodombuilder.cjs.js",
  "module": "build/turbodombuilder.esm.js",
  "browser": "build/turbodombuilder.js",
  "types": "build/turbodombuilder.d.ts",
  "files": [
    "build"
  ],
  "exports": {
    ".": {
      "import": "./build/turbodombuilder.esm.js",
      "require": "./build/turbodombuilder.cjs.js"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "gulp build",
    "doc": "typedoc src/**/*.ts",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Carln02/TurboDOMBuilder.git"
  },
  "keywords": [
    "turbo",
    "fast",
    "DOM",
    "builder",
    "elements",
    "JavaScript",
    "library",
    "simple"
  ],
  "author": "Carln02",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Carln02/TurboDOMBuilder/issues"
  },
  "homepage": "https://carln02.github.io/TurboDOMBuilder/",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "autoprefixer": "^10.4.19",
    "cssnano": "^6.1.2",
    "del": "^7.1.0",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-insert": "^0.5.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.4",
    "gulp-terser": "^2.1.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "rollup": "^4.13.2",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-ignore": "^1.0.10",
    "rollup-plugin-postcss": "^4.0.2",
    "through2": "^4.0.2",
    "ts-loader": "^9.5.1",
    "typedoc": "^0.25.12",
    "typescript": "^5.4.3",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "tslib": "^2.6.2"
  }
}
