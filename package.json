{
  "name": "turbodombuilder",
  "version": "0.3.8",
  "description": "A lightweight JavaScript library for quickly creating and appending DOM elements.",
  "main": "build/cjs/turbodombuilder.js",
  "module": "build/esm/turbodombuilder.js",
  "browser": "build/dist/turbodombuilder.js",
  "types": "build/esm/turbodombuilder.d.ts",
  "files": [
    "build"
  ],
  "exports": {
    ".": {
      "import": "./build/esm/turbodombuilder.js",
      "require": "./build/cjs/turbodombuilder.js"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "gulp build",
    "doc": "typedoc src/**/*.ts",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Carln02/TurboDOMBuilder.git"
  },
  "keywords": [
    "turbo",
    "fast",
    "DOM",
    "builder",
    "elements",
    "JavaScript",
    "library",
    "simple"
  ],
  "author": "Carln02",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Carln02/TurboDOMBuilder/issues"
  },
  "homepage": "https://carln02.github.io/TurboDOMBuilder/",
  "devDependencies": {
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-insert": "^0.5.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.4",
    "gulp-terser": "^2.1.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "through2": "^4.0.2",
    "ts-loader": "^9.5.1",
    "typedoc": "^0.25.8",
    "typescript": "^5.4.3",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  }
}
